@using MG.Models
@model List<Center>
<h1>Center List</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th> 
            Center Name
        </th>
        <th>
            Display Order
        </th>
 

</tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>
                   @obj.DisplayOrder
                </td>
            </tr>
        }
      
    </tbody>
</table>
